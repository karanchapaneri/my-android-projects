            <meta-data android:name="AA_MODELS"
                android:value="com.example.karanc.testapp.Db.Comments, com.example.karanc.testapp.Db.Photos,
                com.example.karanc.testapp.Db.Posts, com.example.karanc.testapp.Db.Todos"/>
              -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>